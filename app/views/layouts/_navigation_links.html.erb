<li><%= link_to 'Vendors', vendors_path %></li>
<li><%= link_to 'About', page_path('about') %></li>
<% if user_signed_in? %>
  <li><%= link_to 'Edit account', edit_user_registration_path %></li>
  <li><%= link_to 'Sign out', destroy_user_session_path, method: 'delete' %></li>
<% elsif vendor_signed_in? %>
	<li><%= link_to 'Edit account', edit_vendor_registration_path %></li>
  <li><%= link_to 'Sign out', destroy_vendor_session_path, method: 'delete' %></li>
<% else %>
  <li><%= link_to 'User Log in', new_user_session_path %></li>
  <li><%= link_to 'Vendor Log in', new_vendor_session_path %></li>
<% end %>
